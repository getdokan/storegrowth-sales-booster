{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sales-booster/sales-countdown",
	"version": "0.1.0",
	"title": "Sales Countdown",
	"attributes": {
		"discountText": {
			"type": "string",
			"default": "Countdown Timer"
		},
		"startDate": {
			"type": "string",
			"default": ""
		},
		"endDate": {
			"type": "string",
			"default": ""
		},
		"headingColor": {
			"type": "string",
			"default": "#008dff"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"borderColor": {
			"type": "string",
			"default": "#008dff"
		}
},
	"category": "widgets",
	"description": "This block shows the countdown of a clock.",
	"supports": {
		"html": false
	},
	"textdomain": "storegrowth-sales-booster",
	"editorScript": "file:../settings.js",
	"editorStyle": "file:./settings.css",
	"render": "file:./render.php",
	"style": "file:./style-index.css"
}
